<Bindings>
  <Binding name="ToggleAddon" description="Hide/Show EchAssistant" header="EchelonedAssistant">
    EchelonedAssistant:ToggleAddon();
  </Binding>
   <Binding name="ToggleChatbox" description="toggles Chatbox" header="Chatbox">
     if ( keystate == "down" ) then
      ToggleChatbox();
      end
  </Binding>
  <Binding name="BIND1" description="Left" header="MYHEADER">
